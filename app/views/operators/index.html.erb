    <!-- Main Content starts -->
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">

      <h2 class="sub-header">Latest Transactions</h2>
      <!--
      <p>
        <div class="btn-group btn-group-sm pull-right" data-toggle="buttons">
          <label class="btn btn-default">
            <input type="radio" name="options" id="pending"> Pending Only
          </label>
          <label class="btn btn-default active">
            <input type="radio" name="options" id="all" checked="checked"> Show All
          </label>
        </div>
        <b>Latest 50 Transactions</b> - <small>Click ID to update transaction</small>
      </p>
      -->
      <br>
      <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
              <th>No.</th>
              <th>ID</th>
              <th>Email Address</th>
              <th>Types</th>
              <th>Amount</th>
              <th>Status</th>
              <th>Remark</th>
            </tr>
          </thead>
          <tbody>
            <%- @report.each_with_index do |t, i| %>
              <% if t.attributes.member?("deposit_date_time") %>
                <tr class="list-group-item-warning">
                  <td><%= (i+1) %></td>
                  <td><a href="/operator/transaction/<%= t.id %>"><b><%= t.id %></b></a><br><%= t.created_at %></td>
                  <td><%= t.user.username %><br><b>Bank name:</b> <%= BankAccount.find(t.bank_account).title %> </td>
                  <td>DEPOSIT</td>
                  <td><%= t.amount %></td>
                  <td><%= t.status %></td>
                  <td><%= t.remark %></td>
                </tr>
              <% elsif t.attributes.member?("from_fund") %>
                <tr class="list-group-item-warning">
                  <td><%= (i+1) %></td>
                  <td><a href="/operator/fund_transfer/<%= t.id %>"><b><%= t.id %></b></a><br><%= t.created_at %></td>
                  <td><%= t.user.username %><br><b>From : </b> <%= t.from_fund %> <br><b>To : </b> <%= t.to_fund %> </td>
                  <td>Fund Transfer</td>
                  <td><%= t.amount %></td>
                  <td><%= t.status %></td>
                  <td><%= t.remark %></td>
                </tr>
              <% else %>
                <tr class="list-group-item-warning">
                  <td><%= (i+1) %></td>
                  <td><a href="/operator/withdraw/<%= t.id %>"><b><%= t.id %></b></a><br><%= t.created_at %></td>
                  <td><%= t.user.username %><br><b>Product:</b> <%= t.title %> </td>
                  <td>WITHDRAWALS</td>
                  <td><%= t.amount %></td>
                  <td><%= t.status %></td>
                  <td><%= t.remark %></td>
                </tr>
            <% end %>
          <% end %>
          </tbody>
          <tfoot>
            <tr class="list-group-item-success">
              <td colspan="4"><center>Total Records: <b>3045</b></center></td>
              <td><b>0.00</b></td>
              <td></td>
              <td>Updated: 4/17/2-14</td>
            </tr>
          </tfoot>
        </table>
      </div>
      <div class="clearfix"></div>
    </div>
